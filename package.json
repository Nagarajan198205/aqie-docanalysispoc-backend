{
  "name": "aqie-docanalysispoc-backend",
  "version": "0.0.0",
  "description": "aqie-docanalysispoc-backend",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=22.16.0"
  },
  "scripts": {
    "build": "npm run build:server",
    "build:server": "NODE_ENV=production babel --delete-dir-on-start --ignore \"**/*.test.js\" --ignore \"**/__fixtures__\" --ignore \"**/test-helpers\" --copy-files --no-copy-ignored --out-dir ./.server ./src",
    "docker:dev": "NODE_ENV=development npm run server:watch",
    "dev": "npm run server:watch",
    "dev:debug": "npm run server:debug",
    "format": "prettier --write \"**/*.{cjs,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{cjs,js,json,md}\"",
    "git:pre-commit-hook": "npm run format:check && npm run lint",
    "lint": "eslint --cache --cache-strategy content \"**/*.{cjs,js}\"",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "npm run setup:husky",
    "server:debug": "nodemon --inspect-brk=0.0.0.0 --ext js,json --legacy-watch ./src",
    "server:watch": "nodemon --inspect=0.0.0.0 --ext js,json --legacy-watch ./src",
    "setup:husky": "node -e \"try { (await import('husky')).default() } catch (e) { if (e.code !== 'ERR_MODULE_NOT_FOUND') throw e }\" --input-type module",
    "start": "NODE_ENV=production node --use-strict .",
    "test": "TZ=UTC vitest run --coverage",
    "test:watch": "TZ=UTC vitest"
  },
  "author": "Defra DDTS",
  "license": "OGL-UK-3.0",
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "3.848.0",
    "@defra/hapi-tracing": "1.22.0",
    "@elastic/ecs-pino-format": "1.5.0",
    "@hapi/boom": "10.0.1",
    "@hapi/hapi": "21.4.0",
    "@hapi/inert": "7.1.0",
    "@langchain/community": "^0.3.49",
    "aws-embedded-metrics": "4.2.0",
    "aws4": "1.13.2",
    "convict": "6.2.4",
    "convict-format-with-validator": "6.2.0",
    "global-agent": "3.0.0",
    "hapi-cors": "1.0.3",
    "hapi-pino": "12.1.0",
    "hapi-pulse": "3.0.1",
    "https-proxy-agent": "7.0.6",
    "joi": "17.13.3",
    "langchain": "^0.1.0",
    "node-fetch": "3.3.2",
    "pdf-parse": "1.1.1",
    "pino": "9.7.0",
    "pino-pretty": "13.0.0",
    "undici": "7.10.0"
  },
  "devDependencies": {
    "@babel/core": "7.27.4",
    "@babel/preset-env": "7.27.2",
    "@vitest/coverage-v8": "3.2.4",
    "autoprefixer": "10.4.21",
    "eslint": "9.29.0",
    "husky": "9.1.7",
    "neostandard": "0.12.1",
    "nodemon": "3.1.10",
    "npm-run-all": "4.1.5",
    "prettier": "3.5.3",
    "vitest": "3.2.4",
    "vitest-fetch-mock": "0.4.5"
  }
}
